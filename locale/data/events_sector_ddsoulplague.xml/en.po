msgid ""
"data/events_sector_ddsoulplague.xml$//ship[@name=\"NEBULA_NEUTRAL_DDSOULPLAGUE_QUIETSILENCE_FIGHT\"]/destroyed/text"
msgstr "The ship breaks apart and you quickly salvage what you can."

#: src-en/data/events_sector_ddsoulplague.xml:3311
msgid ""
"data/events_sector_ddsoulplague.xml$//ship[@name=\"NEBULA_NEUTRAL_DDSOULPLAGUE_QUIETSILENCE_FIGHT\"]/destroyed/choice/text"
