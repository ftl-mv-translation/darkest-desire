msgid ""
"data/events_sector_ddsoulplague.xml$//event[@name=\"STORELIST_DDSOULPLAGUE_SHELLOUTPOST\"]/choice[1]/event/choice[1]/event/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

