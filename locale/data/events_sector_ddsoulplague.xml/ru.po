msgid ""
"data/events_sector_ddsoulplague.xml$//event[@name=\"NEBULA_NEUTRAL_DDSOULPLAGUE_HUNTINGGUILD\"]/choice/event/choice/event/choice[3]/event/choice[2]/text"
msgstr "Ничего не делать."

