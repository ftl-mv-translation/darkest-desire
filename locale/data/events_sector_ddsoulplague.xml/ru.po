"\n"
"\t\t\t\t\t\t\tГЕРОЙ\n"
"\t\t\t\t\t\t\tСМОТРИ\n"
"\t\t\t\t\t\t\tТЫ\n"
"\t\t\t\t\t\t\tСКОРО"

