msgstr ""

#: src-en/data/events_special_lightspeed.xml.append:342
msgid ""
"data/events_special_lightspeed.xml.append$//event[@name=\"ATLAS_EQUIPMENT_DARKGOD_CURSED_BOOK_PERFORMINGRITUAL\"]/choice[5]/text"
