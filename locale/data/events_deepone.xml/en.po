msgstr "Do something onboard the ship."

#: src-en/data/events_deepone.xml:1377
msgid ""
"data/events_deepone.xml$//event[@name=\"ITEMS_DEEPONE_DREAD\"]/choice[1]/event/choice[@req=\"LIST_CREW_DEEPONES\"]/event/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_deepone.xml:1385
msgid ""
"data/events_deepone.xml$//event[@name=\"ITEMS_DEEPONE_DREAD\"]/choice[1]/event/choice[4]/text"
msgstr "This is way too weird. Get out."

#: src-en/data/events_deepone.xml:1387
msgid ""
"data/events_deepone.xml$//event[@name=\"ITEMS_DEEPONE_DREAD\"]/choice[1]/event/choice[4]/event/text"
