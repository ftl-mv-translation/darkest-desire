"data/events_deepone.xml$//event[@name=\"STOREEVENT_DEEPONE_WAR\"]/choice[@req=\"cloaking\"]/text"
msgstr "(Upg. Cloaking) Cloak, and attempt to rob the merchant ship."

#: src-en/data/events_deepone.xml:686
msgid ""
"data/events_deepone.xml$//event[@name=\"STOREEVENT_DEEPONE_WAR\"]/choice[4]/text"
