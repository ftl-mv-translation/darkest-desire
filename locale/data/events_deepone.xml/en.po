msgid ""
"data/events_deepone.xml$//event[@name=\"STOREEVENT_DEEPONE_WAR\"]/choice[4]/event/text"
msgstr "You ignore the merchant ship and are not bothered as the FTL charges."

#: src-en/data/events_deepone.xml:690
msgid ""
"data/events_deepone.xml$//event[@name=\"STOREEVENT_DEEPONE_WAR\"]/choice[4]/event/choice[1]/text"
