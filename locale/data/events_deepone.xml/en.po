msgid ""
"data/events_deepone.xml$//ship[@name=\"NEUTRAL_DDDEEPONE_MATTERVIRUSDEALER_FIGHT\"]/destroyed/text"
msgstr "The ship breaks apart and you quickly salvage what you can."

#: src-en/data/events_deepone.xml:3220
msgid ""
"data/events_deepone.xml$//ship[@name=\"NEUTRAL_DDDEEPONE_MATTERVIRUSDEALER_FIGHT\"]/destroyed/choice/text"
