#: src-en/data/events_deepone.xml:1358
msgid ""
"data/events_deepone.xml$//event[@name=\"ITEMS_DEEPONE_DREAD\"]/choice[1]/event/choice[@req=\"LIST_CREW_DEEPONES\"]/text"
msgstr "(Deep One) Your Deep One looks troubled."

#: src-en/data/events_deepone.xml:1360
msgid ""
"data/events_deepone.xml$//event[@name=\"ITEMS_DEEPONE_DREAD\"]/choice[1]/event/choice[@req=\"LIST_CREW_DEEPONES\"]/event/text"
msgstr ""
"You look to your Deep One for insight into the situation, but they don't "
"seem too keen to provide answers. After pushing a little, they say one word;"
" \"Ritual.\" They insist you leave the Deep Ones here undisturbed. "
"Ominous..."

#: src-en/data/events_deepone.xml:1362
msgid ""
"data/events_deepone.xml$//event[@name=\"ITEMS_DEEPONE_DREAD\"]/choice[1]/event/choice[@req=\"LIST_CREW_DEEPONES\"]/event/choice[1]/text"
msgstr "Push deeper into the outpost."

#: src-en/data/events_deepone.xml:1364
msgid ""
"data/events_deepone.xml$//event[@name=\"ITEMS_DEEPONE_DREAD\"]/choice[1]/event/choice[@req=\"LIST_CREW_DEEPONES\"]/event/choice[1]/event/text"
msgstr ""
