#: src-en/data/events_deepone.xml:741
msgid ""
"data/events_deepone.xml$//event[@name=\"STOREEVENT_DEEPONE_STANDARD\"]/choice[2]/event/text"
msgstr "You've decided to attack the trader."

#: src-en/data/events_deepone.xml:744
msgid ""
"data/events_deepone.xml$//event[@name=\"STOREEVENT_DEEPONE_STANDARD\"]/choice[2]/event/choice/text"
