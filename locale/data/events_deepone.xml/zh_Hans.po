#: src-en/data/events_deepone.xml:3905
msgid ""
"data/events_deepone.xml$//event[@name=\"DISTRESS_DDDEEPONE_SOULPLAGUEINCURSION\"]/choice[3]/event/choice[1]/text"
msgstr "星舰管理。"

#: src-en/data/events_deepone.xml:3909
