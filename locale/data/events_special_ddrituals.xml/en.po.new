msgid ""
"data/events_special_ddrituals.xml$//event[@name=\"ATLAS_EQUIPMENT_DARKGOD_CURSED_BOOK_PERFORMINGRITUAL_DIVINEREMNANTSPERFORMED_PARTSIX_HUMAN\"]/choice[3]/text"
