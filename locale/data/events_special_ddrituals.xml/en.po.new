msgid ""
"data/events_special_ddrituals.xml$//event[@name=\"DDRITUALS_QUESTS_LIST_DIVINEREMNANTS_FIGHT_WIN\"]/choice[@req=\"BLUELIST_DDLIGHTBORNE_DIVINEGIMMICK\"]/text"
