"data/blueprints.xml.append$//shipBlueprint[@name=\"PLAYER_DISPARITY_3\"]/name"
msgstr "Стигийский Пакт"

#: src-en/data/blueprints.xml.append:25556
msgid ""
"data/blueprints.xml.append$//shipBlueprint[@name=\"PLAYER_DISPARITY_3\"]/unlock"
