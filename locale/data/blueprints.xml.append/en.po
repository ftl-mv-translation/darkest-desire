msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"nightmarish_greaterpriest\"]/desc"
msgstr ""
"A Greater variant of the Nightmarish Priest, it will see to it that your "
"horde doesn't fall."

#: src-en/data/blueprints.xml.append:17369
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"nightmarish_greaterpriest\"]/title"
msgstr "Nightmarish Greater Priest"

#: src-en/data/blueprints.xml.append:17370
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"nightmarish_greaterpriest\"]/short"
