msgid ""
"data/events_enlightened.xml$//event[@name=\"ITEMS_ENLIGHTENED_ENGI_INVESTIGATION\"]/choice/event/choice[1]/event/choice[1]/event/choice/event/choice[2]/event/choice[2]/event/text"
msgstr ""
