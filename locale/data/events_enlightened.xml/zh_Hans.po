msgid ""
"data/events_enlightened.xml$//event[@name=\"STORE_HOSTILE_ENLIGHTENED_GAMBLING\"]/choice[1]/event/choice[2]/text"
msgstr "攻击！"

#: src-en/data/events_enlightened.xml:29
msgid ""
"data/events_enlightened.xml$//event[@name=\"STORE_HOSTILE_ENLIGHTENED_GAMBLING\"]/choice[1]/event/choice[2]/event/text"
msgstr "启蒙者商人察觉到了你的敌意，准备自卫。"

#: src-en/data/events_enlightened.xml:33
msgid ""
"data/events_enlightened.xml$//event[@name=\"STORE_HOSTILE_ENLIGHTENED_GAMBLING\"]/choice[1]/event/choice[2]/event/choice/text"
msgstr "继续。"

#: src-en/data/events_enlightened.xml:41
msgid ""
"data/events_enlightened.xml$//event[@name=\"STORE_HOSTILE_ENLIGHTENED_GAMBLING\"]/choice[2]/text"
msgstr "继续吧，我要离开这里了。"

#: src-en/data/events_enlightened.xml:43
msgid ""
"data/events_enlightened.xml$//event[@name=\"STORE_HOSTILE_ENLIGHTENED_GAMBLING\"]/choice[2]/event/text"
msgstr "“明白。”声音沉寂下去，只留下你独自置身于黑暗之中。"

#: src-en/data/events_enlightened.xml:45
msgid ""
"data/events_enlightened.xml$//event[@name=\"STORE_HOSTILE_ENLIGHTENED_GAMBLING\"]/choice[2]/event/choice[1]/text"
msgstr "检查储物舱。"

#: src-en/data/events_enlightened.xml:49
msgid ""
"data/events_enlightened.xml$//event[@name=\"STORE_HOSTILE_ENLIGHTENED_GAMBLING\"]/choice[2]/event/choice[2]/text"
