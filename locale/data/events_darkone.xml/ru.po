"\t\t\t\t\t\t\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\tТОТ ОТДАЛЕННЫЙ, ТУМАННЫЙ ПЛАН - РАНЬШЕ СЧИТАВШИЙСЯ ПОТЕРЯННЫМ, НО ТЕПЕРЬ ВОССТАНОВЛЕННЫЙ.\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\tИМЕННО С ЭТОЙ ЗЕМЛИ ДЕРЕВЬЕВ ЛЬЕТСЯ ПЕСНЯ - ЕЕ НОТЫ РАЗНОСЯТСЯ ПО ВСЕЙ МУЛЬТИВСЕЛЕННОЙ.\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\tХОТЯ НЕЛЬЗЯ УВИДЕТЬ ЕЕ КРАСОТУ И ПОНЮХАТЬ ЕЕ РОЗЫ... РЕДКО КТО МОЖЕТ УСЛЫШАТЬ ЕЕ ПЕСНЮ.\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\tЕСЛИ ОНИ БУДУТ СЛУШАТЬ СРЕДИ МОРЯ ТЕНЕЙ И ДОКАЖУТ СИЛУ СВОЕЙ СОБСТВЕННОЙ МЕЛОДИИ...\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\n"
