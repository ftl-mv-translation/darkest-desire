#: src-en/data/events_disparity.xml:2792
msgid ""
"data/events_disparity.xml$//event[@name=\"NEUTRAL_DISPARITY_ENLIGHTENED\"]/choice[@req=\"deeponecultist\"]/event/choice[4]/event/choice[3]/event/text"
msgstr ""
