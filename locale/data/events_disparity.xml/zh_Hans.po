msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_CRYSTAL\"]/choice[1]/event/choice[2]/event/choice[@req=\"LIST_CREW_CRYSTAL\"]/event/choice/event/choice[8]/text"
msgstr "就我个人而言，我毫无头绪。"

#: src-en/data/events_disparity.xml:655
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_CRYSTAL\"]/choice[1]/event/choice[2]/event/choice[@req=\"LIST_CREW_CRYSTAL\"]/event/choice/event/choice[8]/event/text"
msgstr "水晶戍卫者叹了口气，“不，我想你也不会知道。这件事你就别对外声张了，还有……谢谢"
