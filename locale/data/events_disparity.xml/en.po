msgid ""
"data/events_disparity.xml$//event[@name=\"NEUTRAL_DISPARITY_SHADOWSTORM_APPROACHES\"]/choice/event/choice[1]/event/choice/event/choice[1]/event/choice[1]/event/choice/event/choice[1]/event/choice[1]/event/choice/event/choice/event/choice/event/choice[1]/text"
