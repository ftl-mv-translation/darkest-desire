#: src-en/data/events_disparity.xml:389
msgid ""
"data/events_disparity.xml$//ship[@name=\"STORELIST_DISPARITY_DISPARITY_ASSAULT_FIGHT_DISPARITY\"]/deadCrew/text"
msgstr "With the crew dead you quickly salvage what you can."

#: src-en/data/events_disparity.xml:392
msgid ""
"data/events_disparity.xml$//ship[@name=\"STORELIST_DISPARITY_DISPARITY_ASSAULT_FIGHT_DISPARITY\"]/deadCrew/choice/text"
msgstr "Continue..."

#: src-en/data/events_disparity.xml:399
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_DISPARITY_ASSAULT_FIGHT_DISPARITY_WIN\"]/text"
msgstr ""
"You may have defeated the Disparity ship directly on top of you, but you're "
"still caught in the crossfire! Get out of here as soon as you can!"

#: src-en/data/events_disparity.xml:405
msgid ""
"data/events_disparity.xml$//ship[@name=\"STORELIST_DISPARITY_DISPARITY_ASSAULT_FIGHT_HARMONY\"]/destroyed/text"
msgstr "The ship breaks apart and you quickly salvage what you can."

#: src-en/data/events_disparity.xml:408
msgid ""
"data/events_disparity.xml$//ship[@name=\"STORELIST_DISPARITY_DISPARITY_ASSAULT_FIGHT_HARMONY\"]/destroyed/choice/text"
