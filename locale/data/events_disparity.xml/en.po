#: src-en/data/events_disparity.xml:248
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_DISPARITY_NORMAL_FIGHT_WIN\"]/text"
msgstr ""
"The other ships have already gotten away. A pity, but you proved your might."

#: src-en/data/events_disparity.xml:256
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_DISPARITY_NORMAL\"]/text"
msgstr ""
"A group of Disparity ships are huddled around this beacon. They don't really"
" acknowledge your presence... they're likely busy trading amongst "
"themselves."

#: src-en/data/events_disparity.xml:258
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_DISPARITY_NORMAL\"]/choice[@req=\"dd_disparity_assistance\""
" and @lvl=\"-99\"]/text"
msgstr "Try to get in on the action and trade."

#: src-en/data/events_disparity.xml:260
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_DISPARITY_NORMAL\"]/choice[@req=\"dd_disparity_assistance\""
" and @lvl=\"-99\"]/event/text"
msgstr ""
"You try to hail the huddled ships, but they don't pick up. They mustn't "
"fancy your presence here..."

#: src-en/data/events_disparity.xml:262
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_DISPARITY_NORMAL\"]/choice[@req=\"dd_disparity_assistance\""
" and @lvl=\"-99\"]/event/choice[1]/text"
msgstr "Try hailing them again."

#: src-en/data/events_disparity.xml:264
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_DISPARITY_NORMAL\"]/choice[@req=\"dd_disparity_assistance\""
" and @lvl=\"-99\"]/event/choice[1]/event/text"
msgstr "Again, your hail goes unanswered."

