msgstr ""
"A voice speaks to you from the shadows that surround you, \"TRULY, I AM "
"SORRY.\""

#: src-en/data/events_disparity.xml:3160
msgid ""
"data/events_disparity.xml$//event[@name=\"NEUTRAL_DISPARITY_SHADOWSTORM_APPROACHES\"]/choice/event/choice[1]/event/choice/event/choice[1]/event/choice[1]/event/choice/event/choice[1]/event/choice[1]/event/choice/event/choice/event/choice/event/choice[2]/event/choice/event/choice/text"
