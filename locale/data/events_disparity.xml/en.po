msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_HARMONY\"]/choice[4]/text"
msgstr "Stay low, and out of sight."

#: src-en/data/events_disparity.xml:77
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_HARMONY\"]/choice[4]/event/text"
msgstr ""
"You're able to successfully evade the guards' sight as you wait for the "
"engines to charge."

#: src-en/data/events_disparity.xml:79
msgid ""
"data/events_disparity.xml$//event[@name=\"STORELIST_DISPARITY_HARMONY\"]/choice[4]/event/choice[1]/text"
