#: src-en/data/events_disparity.xml:2004
msgid ""
"data/events_disparity.xml$//event[@name=\"NEUTRAL_DISPARITY_BLOODSHED\"]/choice[1]/event/choice/event/choice[2]/event/choice[2]/event/choice[@req=\"LIST_CREW_DISPARITY\"]/event/choice[2]/event/choice/event/choice/event/choice/text"
msgstr "No, not their rights, their writes!"

#: src-en/data/events_disparity.xml:2006
msgid ""
"data/events_disparity.xml$//event[@name=\"NEUTRAL_DISPARITY_BLOODSHED\"]/choice[1]/event/choice/event/choice[2]/event/choice[2]/event/choice[@req=\"LIST_CREW_DISPARITY\"]/event/choice[2]/event/choice/event/choice/event/choice/event/text"
msgstr ""
