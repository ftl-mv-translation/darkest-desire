#: src-en/data/events_enlightened.xml:2742
msgid ""
"data/events_enlightened.xml$//event[@name=\"ITEMS_ENLIGHTENED_ENGI_INVESTIGATION\"]/choice/event/choice[1]/event/choice[1]/event/choice/event/choice[1]/event/choice[3]/text"
msgstr "Demander des missiles."

#: src-en/data/events_enlightened.xml:2744
msgid ""
"data/events_enlightened.xml$//event[@name=\"ITEMS_ENLIGHTENED_ENGI_INVESTIGATION\"]/choice/event/choice[1]/event/choice[1]/event/choice/event/choice[1]/event/choice[3]/event/text"
