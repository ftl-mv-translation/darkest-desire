msgid ""
"data/events_special_ddrituals.xml$//event[@name=\"ATLAS_EQUIPMENT_DARKGOD_CURSED_BOOK_PERFORMINGRITUAL_AGEDFISHPERFORMED\"]/choice[@req=\"DDHIDDENAUG_TRIBUTEMASK_SHATTER\""
