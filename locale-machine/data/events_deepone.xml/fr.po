msgstr "Taper du pied, et porter l'enchère à 505."

#: src-en/data/events_deepone.xml:1868
msgid ""
"data/events_deepone.xml$//event[@name=\"ITEMS_DDDEEPONE_TEMPESTAUCTION_BIDSTART_HIGH_STEADY_UNLUCKY\"]/choice[1]/event/text"
msgstr ""
